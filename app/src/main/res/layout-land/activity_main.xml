<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/h" />


    <RelativeLayout
        android:id="@+id/board"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:scaleType="centerCrop">

        <TextView
            android:id="@+id/view11"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="2dp"
            android:background="@drawable/image_part_001"
            android:gravity="center"
            android:text="1"
            android:textColor="@color/buttext"
            android:textSize="42sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/view12"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="2dp"
            android:layout_toEndOf="@+id/view11"
            android:layout_toRightOf="@+id/view11"
            android:background="@drawable/image_part_002"
            android:gravity="center"
            android:text="2"
            android:textColor="@color/buttext"
            android:textSize="42sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/view13"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="2dp"
            android:layout_toEndOf="@+id/view12"
            android:layout_toRightOf="@+id/view12"
            android:background="@drawable/image_part_003"
            android:gravity="center"
            android:text="3"
            android:textAlignment="center"
            android:textColor="@color/buttext"
            android:textSize="42sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/view14"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="2dp"
            android:layout_toEndOf="@+id/view13"
            android:layout_toRightOf="@+id/view13"
            android:background="@drawable/image_part_004"
            android:gravity="center"
            android:text="4"
            android:textAlignment="center"
            android:textColor="@color/buttext"
            android:textSize="42sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/view21"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="2dp"
            android:layout_below="@+id/view11"
            android:background="@drawable/image_part_005"
            android:gravity="center"
            android:text="5"
            android:textAlignment="center"
            android:textColor="@color/buttext"
            android:textSize="42sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/view22"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="2dp"
            android:layout_below="@+id/view12"
            android:layout_toEndOf="@+id/view21"
            android:background="@drawable/image_part_006"
            android:gravity="center"
            android:text="6"
            android:textAlignment="center"
            android:textColor="@color/buttext"
            android:textSize="42sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/view23"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="2dp"
            android:layout_below="@+id/view13"
            android:layout_toEndOf="@+id/view22"
            android:background="@drawable/image_part_007"
            android:gravity="center"
            android:text="7"
            android:textAlignment="center"
            android:textColor="@color/buttext"
            android:textSize="42sp"
            android:textStyle="bold" />


        <TextView
            android:id="@+id/view24"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="2dp"
            android:layout_below="@id/view14"
            android:layout_toEndOf="@+id/view23"
            android:background="@drawable/image_part_008"
            android:gravity="center"
            android:text="8"
            android:textAlignment="center"
            android:textColor="@color/buttext"
            android:textSize="42sp"
            android:textStyle="bold" />


        <TextView
            android:id="@+id/view31"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="2dp"
            android:layout_below="@id/view21"
            android:background="@drawable/image_part_009"
            android:gravity="center"
            android:text="9"
            android:textAlignment="center"
            android:textColor="@color/buttext"
            android:textSize="42sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/view32"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="2dp"
            android:layout_below="@id/view22"
            android:layout_toEndOf="@+id/view31"
            android:background="@drawable/image_part_010"
            android:gravity="center"
            android:text="10"
            android:textAlignment="center"
            android:textColor="@color/buttext"
            android:textSize="42sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/view33"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="2dp"
            android:layout_below="@id/view23"
            android:layout_toEndOf="@+id/view32"
            android:background="@drawable/image_part_011"
            android:gravity="center"
            android:text="11"
            android:textAlignment="center"
            android:textColor="@color/buttext"
            android:textSize="42sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/view34"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="2dp"
            android:layout_below="@id/view24"
            android:layout_toEndOf="@+id/view33"
            android:background="@drawable/image_part_012"
            android:gravity="center"
            android:text="12"
            android:textAlignment="center"
            android:textColor="@color/buttext"
            android:textSize="42sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/view41"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="2dp"
            android:layout_below="@id/view31"
            android:background="@drawable/image_part_013"
            android:gravity="center"
            android:text="13"
            android:textAlignment="center"
            android:textColor="@color/buttext"
            android:textSize="42sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/view42"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="2dp"
            android:layout_below="@id/view32"
            android:layout_toEndOf="@+id/view41"
            android:background="@drawable/image_part_014"
            android:gravity="center"
            android:text="14"
            android:textAlignment="center"
            android:textColor="@color/buttext"
            android:textSize="42sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/view43"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="2dp"
            android:layout_below="@id/view33"
            android:layout_toEndOf="@+id/view42"
            android:background="@drawable/image_part_015"
            android:gravity="center"
            android:text="15"
            android:textAlignment="center"
            android:textColor="@color/buttext"
            android:textSize="42sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/view44"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="2dp"
            android:layout_below="@id/view34"
            android:layout_toEndOf="@+id/view43"
            android:background="@drawable/image_part_016"
            android:gravity="center"
            android:textSize="42sp"
            android:textStyle="bold"
            android:textColor="@color/buttext"
            tools:text="" />


    </RelativeLayout>
    <Button
        android:id="@+id/newGame"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/new_game"
        android:visibility="invisible"
        android:layout_centerInParent="true"
        />
</RelativeLayout>
